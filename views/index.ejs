<% layout('./layout/default') %>
<div id="fh5co-main">
  <div class="container">
    <div class="row">
      <div id="fh5co-board" data-columns>
      <% data.forEach(function(item){ %>
        <div class="item">
          <div class="animate-box">
            <a href="/uploads/<%= item.name %>" class="image-popup fh5co-board-img" title="<%= item.title %>">
              <img src="/uploads/<%= item.name %>" alt="Free HTML5 Bootstrap template">
            </a>
            <div class="fh5co-desc"><%= item.title %></div>
          </div>
        </div>
      <% }) %>
      </div>
    </div>
  </div>
</div>